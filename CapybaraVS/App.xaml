            <SolidColorBrush x:Key="CurvePathCastConnectableBrush" Color="Tomato"/>
            <SolidColorBrush x:Key="CurvePathDuringBrush" Color="DeepSkyBlue"/>
            <SolidColorBrush x:Key="ConstructorNodeBackgroundBrush" Color="#66ADFF2F"/>
            <SolidColorBrush x:Key="ConstructorTypeBackgroundBrush" Color="LightSalmon"/>
            <SolidColorBrush x:Key="EntryPointNodeBackgroundBrush" Color="#99BCBCFF"/>
            <SolidColorBrush x:Key="NormalNodeBackgroundBrush" Color="#99FFF5EE"/>
            <SolidColorBrush x:Key="NotUseCacheNormalNodeStrokeBrush" Color="#66909090"/>
            <SolidColorBrush x:Key="NormalNodeStrokeBrush" Color="#660000fa"/>
            <SolidColorBrush x:Key="LinkedConnectorStrokeBrush" Color="CornflowerBlue"/>
            <SolidColorBrush x:Key="UnlinkedConnectorStrokeBrush" Color="Blue"/>
