<Window x:Class="CapyCSS.OutputWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CapyCSS"
        mc:Ignorable="d"
        Title="OutputWindow" Height="378" Width="424">
    <Grid>
        <TextBox x:Name="Log" TextWrapping="Wrap" Text="{Binding BindText}" VerticalScrollBarVisibility="Auto" IsReadOnly="True" Background="Black" Foreground="White" />
    </Grid>
</Window>
